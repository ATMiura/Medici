                                            <use xlink:href="#icon-point"></use>
                                        </svg>
                                    </i>
                                    <span class="circle-item__label circle-item__label--uppercase">Краснодар</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
